jsCodeInForm="
(function(){
	var f = $('#$$idForm$$').wigii('FormHelper');
	var dq = wigii().txtQuot;
	var onRecurrenceUnitChange = function(){
		$('
			#$$idForm$$__weekDays,
			#$$idForm$$__monthlyType,
			#$$idForm$$__monthlyDetailGroup,
			#$$idForm$$__yearlyDay,
			#$$idForm$$__yearlyMonth
			').hide();
		if(f.val('recurrenceUnit')=='week'){
			$('
			#$$idForm$$__weekDays
			').show();
		}
		if(f.val('recurrenceUnit')=='month'){
			$('
			#$$idForm$$__monthlyType,
			#$$idForm$$__monthlyDetailGroup
			').show();
		}
		if(f.val('recurrenceUnit')=='year'){
			$('
			#$$idForm$$__yearlyDay,
			#$$idForm$$__yearlyMonth
			').show();
		}
		
	};
	$('#$$idForm$$__recurrenceUnit').change(onRecurrenceUnitChange);
	onRecurrenceUnitChange();
})();"
