(function(){
	/* default to one year on */
	var f = $('#$$idForm$$').wigii('FormHelper');
	var dq = wigii().txtQuot;
	var onStartDateChange = function(){
		if(!f.val('endDate')){
			f.val('endDate', moment(f.val('startDate'),'DD.MM.YYYY').add('month','6').format('DD.MM.YYYY'));
		} else if(moment(f.val('startDate'),'DD.MM.YYYY').unix()>moment(f.val('endDate'),'DD.MM.YYYY').unix()){
			f.val('endDate','');
		}
	};
	$('#$$idForm$$__startDate').change(onStartDateChange);
	onStartDateChange();
})();">
